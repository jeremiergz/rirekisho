{
  "name": "rirekisho",
  "version": "21.12.4",
  "private": true,
  "description": "Yet Another Resume Website. This time it's mine though.",
  "keywords": [
    "cv",
    "gatsby.js",
    "react",
    "resume",
    "rirekisho",
    "tailwindcss",
    "typescript"
  ],
  "homepage": "https://cv.jeremierodriguez.com",
  "bugs": {
    "url": "https://github.com/jeremiergz/rirekisho/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremiergz/rirekisho.git"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    ".cache/",
    "public/",
    "package-lock.json"
  ],
  "scripts": {
    "build": "run-s clean build:compile",
    "build:compile": "gatsby build",
    "clean": "rimraf .cache/ coverage/ package/ public/ rirekisho-*.tgz *.tsbuildinfo",
    "deploy": "gh-pages --dist public --repo https://${GH_TOKEN}@github.com/jeremiergz/rirekisho.git",
    "lint": "run-p lint:eslint lint:tsc",
    "lint:eslint": "eslint --ext ts,tsx src",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky install",
    "prerelease": "run-s build lint test",
    "release": "node release.js",
    "start": "gatsby serve",
    "start:debug": "NODE_OPTIONS=\"--inspect --max_old_space_size=4096\" gatsby develop",
    "start:w": "NODE_OPTIONS=--max_old_space_size=4096 gatsby develop",
    "test": "jest",
    "test:cc": "jest --clearCache",
    "version": "node --print process.env.npm_package_version"
  },
  "dependencies": {
    "@ctrl/tinycolor": "3.4.0",
    "clsx": "1.1.1",
    "gatsby": "4.4.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-image": "2.4.0",
    "gatsby-plugin-manifest": "4.4.0",
    "gatsby-plugin-offline": "5.4.0",
    "gatsby-plugin-postcss": "5.4.0",
    "gatsby-plugin-react-helmet": "5.4.0",
    "gatsby-plugin-robots-txt": "1.6.14",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sharp": "4.4.0",
    "gatsby-plugin-sitemap": "5.4.0",
    "gatsby-source-filesystem": "4.4.0",
    "gatsby-transformer-json": "4.4.0",
    "gatsby-transformer-sharp": "4.4.0",
    "lodash.throttle": "4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@kobionic/eslint-config-typescript": "^4.0.3",
    "@kobionic/eslint-config-typescript-react": "^2.0.3",
    "@types/jest": "^27.0.3",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^2.2.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "babel-preset-gatsby": "^2.4.0",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.6",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16"
  }
}
